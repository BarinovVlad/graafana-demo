{
  "uid": "go-runtime-dashboard001",
  "title": "Go Runtime Metrics",
  "tags": ["prometheus", "go"],
  "timezone": "browser",
  "schemaVersion": 30,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "id": 4,
      "libraryPanel": {
        "name": "Pie chart",
        "uid": "eevmw4959ze9se"
      },
      "title": "Pie chart",
      "type": "library-panel-ref"
    },
    {
      "type": "graph",
      "title": "Go GC Pause Count",
      "id": 1,
      "datasource": "prometheus",
      "targets": [
        {
          "expr": "increase(go_gc_pauses_seconds_count[5m])",
          "legendFormat": "GC Pauses (5m)",
          "refId": "A"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Heap Allocation (bytes)",
      "id": 2,
      "datasource": "prometheus",
      "targets": [
        {
          "expr": "go_memstats_heap_alloc_bytes",
          "legendFormat": "Heap Alloc",
          "refId": "B"
        }
      ]
    }
  ]
}
